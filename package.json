{
  "name": "@pickme/style-guide",
  "version": "1.6.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite build --watch\" \"vite preview\"",
    "build": "vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "storybook:vue": "storybook dev -c src/vue-components/.storybook -p 6007",
    "storybook:react": "storybook dev -c src/react-components/.storybook -p 6008",
    "build-storybook:react": "cd src/react-components && npx storybook build -o ../../dist/react",
    "build-storybook:vue": "cd src/vue-components && npx storybook build -o ../../dist/vue",
    "build-storybook": "storybook build --output-dir dist"
  },
  "author": "",
  "license": "ISC",
  "description": "PickMe 서비스의 공통 스타일 가이드",
  "dependencies": {
    "@chakra-ui/react": "^3.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@originjs/vite-plugin-federation": "^1.3.9",
    "@storybook/addon-docs": "^8.6.4",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-onboarding": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/experimental-addon-test": "^8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/react-vite": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@storybook/vue3": "^8.6.4",
    "@storybook/vue3-vite": "^8.6.4",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/browser": "^3.0.8",
    "@vitest/coverage-v8": "^3.0.8",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "playwright": "^1.51.0",
